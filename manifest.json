{
  "manifest_version": 2,
  "name": "Stedin Header 45 graden",
  "version": "1.1",
  "description": "ProjectPortaal header 45 graden.",

  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
